{
	"name": "gulpstrap",
	"version": "2.0.0",
	"description": "Generate your static website with Bootstrap 5.1, FontAwesome 5, jQuery, and Popper included. Very nice for prototyping. Supports multiple theme styling.",
	"license": "MIT",
	"private": false,
	"repository": {
		"type": "git",
		"url": "git@github.com:vr1e/gulpstrap.git"
	},
	"engines": {
		"node": "22"
	},
	"scripts": {
		"start": "gulp",
		"build": "gulp build"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"browser-sync": "^3.0.4",
		"gulp": "^5.0.1",
		"gulp-clean-css": "^4.3.0",
		"gulp-concat": "^2.6.1",
		"gulp-rename": "^2.1.0",
		"gulp-sass": "^6.0.1",
		"gulp-uglify": "^3.0.2",
		"sass": "^1.93.2"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"bootstrap": "^5.3.8",
		"jquery": "^3.7.1"
	},
	"prettier": {
		"trailingComma": "none",
		"useTabs": true,
		"tabWidth": 2,
		"semi": true,
		"singleQuote": true,
		"bracketSpacing": true,
		"arrowParens": "avoid"
	}
}
